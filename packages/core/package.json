{
  "name": "@unlike/github-actions-core",
  "version": "0.0.4",
  "description": "Actions core lib",
  "keywords": [
    "github",
    "actions",
    "core"
  ],
  "homepage": "https://github.com/unlike-ltd/github-actions-toolkit/tree/main/packages/core",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "node": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "types": "./dist/index.d.cts",
    "default": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlike-ltd/github-actions-toolkit.git",
    "directory": "packages/core"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "rm -rf dist && tsup-node",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "bugs": {
    "url": "https://github.com/unlike-ltd/github-actions-toolkit/issues"
  },
  "engines": {
    "node": ">=v16.20.0"
  },
  "dependencies": {
    "@actions/http-client": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "16.18.36",
    "tsconfig": "workspace:*",
    "tsup": "7.1.0",
    "typescript": "5.1.3"
  }
}
